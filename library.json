{
    "name": "daisysp",
    "version": "0.0.1",
    "build": {
      "srcDir": "./Source",
      "extraScript": "./platformio.py",
      "srcFilter": [
        "+<Control>",
        "+<Drums>",
        "+<Dynamics>",
        "+<Effects>",
        "+<Filters>",
        "+<Noise>",
        "+<PhysicalModeling>",
        "+<Synthesis>",
        "+<Utility>"
      ],
  
      "flags": [
        "-O3",
        "-static",
        "-ISource",
        "-IControl",
        "-IDrums",
        "-IDynamics",
        "-IEffects",
        "-IFilters",
        "-INoise",
        "-IPhysicalModeling",
        "-ISynthesis",
        "-IUtility"
      ],
      "unflags": ["-Os"]
    }
  }
